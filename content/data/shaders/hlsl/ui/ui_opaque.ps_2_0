struct INTERPOLANTS
{
    float4 position       : POSITION;
    float4 perVertexColor : COLOR0;
    float2 tex0           : TEXCOORD0;
};

float4 main(INTERPOLANTS interpolants) : COLOR
{
    float3 col = tex2D(UI_PRIMARY_TEXTURE, interpolants.tex0).rgb * interpolants.perVertexColor.rgb * UI_COLOR.rgb;
    return float4(col, 1);
}
